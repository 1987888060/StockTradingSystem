<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jsu.per.system.dao.BuyedStockMapper" >
    <select id="getBuyed" parameterType="int" resultType="jsu.per.system.pojo.StockSimple">
        SELECT DISTINCT  stockcode,stockname FROM `historytrade` WHERE userid = #{userid}
    </select>
</mapper>